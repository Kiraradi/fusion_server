{
  "name": "fusion_server",
  "version": "1.0.0",
  "description": "My server",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --files src/index.ts",
    "test": "",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:up": "npm run typeorm migration:run -- -d ./src/database/dataSource.ts",
    "migration:sync": "npm run typeorm -- -d ./src/database/dataSource.ts migration:generate ./src/database/migrations/sync",
    "migration:down": "npm run typeorm -- -d ./src/database/dataSource.ts migration:revert",
    "migration:show": "npm run typeorm -- migration:show -d ./src/database/dataSource.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kiraradi/fusion_server.git"
  },
  "author": "",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/express": "5.0.0",
    "@types/node": "16.11.10",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "typescript-eslint": "^8.8.1"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "express": "4.21.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.4.0",
    "typeorm": "0.3.20",
    "yup": "^1.4.0"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kiraradi/fusion_server/issues"
  },
  "homepage": "https://github.com/Kiraradi/fusion_server#readme"
}
